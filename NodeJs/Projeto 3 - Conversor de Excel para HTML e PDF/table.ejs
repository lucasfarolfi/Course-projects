<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Conversor de Excel para HTML</title>
</head>
<body>
    <table>

        <!-- Exibe as colunas da linha do cabeÃ§alho -->
        <thead>
            <% header.forEach(col=>{ %>
                <th><%= col %></th>
            <% }) %>
        </thead>

        <!-- Cria uma linha tr em cada linha da rows, e depois cria varias colunas em cada row -->
        <tbody>
            <% rows.forEach(row=>{ %>
                <tr>
                    <% row.forEach(col =>{ %>
                        <td><%= col %></td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>